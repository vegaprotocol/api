{
  "dependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.1.4",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "graphql": "15.5.2",
    "graphqldoc": "2.5.2",
    "marked": "3.0.3",
    "ws": "8.2.2"
  },
  "scripts": {
    "docs": "graphqldoc -f -s schema.graphql -o doc/ && find doc -name '*.html' -print0 | xargs -0r sed --in-place -e 's#><#>\\n<#g'",
